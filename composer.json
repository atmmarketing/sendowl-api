{
  "name": "atmmarketing/sendowl-api",
  "description": "PHP interface for Send Owl API integration",
  "minimum-stability": "dev",
  "license": "MIT",
  "authors": [
    {
      "name": "Ricardo Lewis",
      "email": "webdev@atmmarketing.com.au"
    }
  ],
  "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/roave/security-advisories"
        }
    ], 
  "require-dev": {
    "requests/test-server": "dev-master"
  },
  "require": {
    "php": "~5.5",
    "roave/security-advisories":"dev-master",
    "rmccue/requests": ">=1.0"
  },
  "config": {
    "bin-dir": "bin"
  },
  "autoload": {
    "psr-4": {
      "AtmMarketing\\SendOwlAPI\\": "src/"
      }
    },
  "autoload-dev": {
    "psr-4": {
      "spec\\AtmMarketing\\SendOwlAPI\\": "spec/"
    }
  }
}
